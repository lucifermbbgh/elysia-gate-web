#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/JavaProject/elysia-gate-web/node_modules/import-local/fixtures/node_modules:/mnt/c/JavaProject/elysia-gate-web/node_modules/import-local/node_modules:/mnt/c/JavaProject/elysia-gate-web/node_modules:/mnt/c/JavaProject/node_modules:/mnt/c/node_modules:/mnt/c/JavaProject/elysia-gate-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/JavaProject/elysia-gate-web/node_modules/import-local/fixtures/node_modules:/mnt/c/JavaProject/elysia-gate-web/node_modules/import-local/node_modules:/mnt/c/JavaProject/elysia-gate-web/node_modules:/mnt/c/JavaProject/node_modules:/mnt/c/node_modules:/mnt/c/JavaProject/elysia-gate-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../import-local/fixtures/cli.js" "$@"
fi
